{
  "name": "slonik-live-server",
  "version": "0.0.0-development",
  "description": "slonik live server monorepo",
  "packageManager": "yarn@3.5.1",
  "private": true,
  "keywords": [
    "slonik",
    "live server"
  ],
  "bugs": {
    "url": "https://github.com/hoonoh/slonik-live-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoonoh/slonik-live-server"
  },
  "author": {
    "name": "hoonoh"
  },
  "license": "MIT",
  "scripts": {
    "release": "lerna run release",
    "build": "lerna run build",
    "prepare": "husky install && lerna run prepare && lerna link",
    "postinstall": "lerna run postinstall && lerna link",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "test:ci": "lerna run test:ci",
    "types": "lerna run types"
  },
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@swc/core": "1.3.57",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.8",
    "@types/pg": "8.6.6",
    "@types/vscode": "1.78.0",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vscode/vsce": "2.19.0",
    "chalk": "5.2.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-extended": "3.2.4",
    "lerna": "6.6.2",
    "mockdate": "3.0.5",
    "prettier": "2.8.8",
    "semantic-release": "19.0.5",
    "semantic-release-monorepo": "7.0.5",
    "semantic-release-vsce": "5.5.9",
    "ts-essentials": "9.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@babel/core": "7.21.8",
    "@vscode/vsce@2.19.0": "patch:@vscode/vsce@npm%3A2.19.0#./.yarn/patches/@vscode-vsce-npm-2.19.0-d912dcb0c4.patch",
    "minimist": "1.2.6",
    "xml2js": "0.5.0"
  }
}
