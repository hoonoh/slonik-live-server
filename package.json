{
  "name": "slonik-live-server",
  "version": "0.0.0-development",
  "description": "slonik live server monorepo",
  "packageManager": "yarn@3.6.3",
  "private": true,
  "keywords": [
    "slonik",
    "live server"
  ],
  "bugs": {
    "url": "https://github.com/hoonoh/slonik-live-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoonoh/slonik-live-server"
  },
  "author": {
    "name": "hoonoh"
  },
  "license": "MIT",
  "scripts": {
    "release": "lerna run release",
    "build": "lerna run build",
    "prepare": "husky install && lerna run prepare && lerna link",
    "postinstall": "lerna run postinstall && lerna link",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "test:ci": "lerna run test:ci",
    "types": "lerna run types"
  },
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.4",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "10.0.5",
    "@semantic-release/release-notes-generator": "11.0.7",
    "@swc/core": "1.3.83",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.4",
    "@types/node": "18.17.14",
    "@types/pg": "8.10.2",
    "@types/vscode": "1.82.0",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "@vscode/vsce": "2.21.0",
    "chalk": "5.3.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.48.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-extended": "4.0.1",
    "lerna": "6.6.2",
    "mockdate": "3.0.5",
    "prettier": "2.8.8",
    "semantic-release": "21.1.1",
    "semantic-release-monorepo": "7.0.5",
    "semantic-release-vsce": "5.6.1",
    "ts-essentials": "9.4.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "@babel/core": "7.22.17",
    "@vscode/vsce@2.19.0": "patch:@vscode/vsce@npm%3A2.19.0#./.yarn/patches/@vscode-vsce-npm-2.19.0-d912dcb0c4.patch",
    "minimist": "1.2.8",
    "xml2js": "0.6.2"
  }
}
